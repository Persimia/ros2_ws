<?xml version='1.0'?>
<sdf version='1.9'>
  <model name='turbine'>
    <static>true</static>
    <pose>0 0 0 0 0 0</pose>
    <link name='tower_link'>
      <visual name='tower_visual'>
        <geometry>
          <mesh>
            <uri>model://turbine/meshes/tower.stl</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.05 0.05 0.05</ambient>
          <diffuse>0.05 0.05 0.05</diffuse>
          <specular>1 1 1 1</specular>
          <pbr>
            <metal>
              <metalness>0.5</metalness>
              <roughness>0.5</roughness>
            </metal>
          </pbr>
        </material>
      </visual>
    </link>
    <link name='rotor_link'>
      <visual name='rotor_visual'>
        <geometry>
          <mesh>
            <uri>model://turbine/meshes/rotor.stl</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.05 0.05 0.05</ambient>
          <diffuse>0.05 0.05 0.05</diffuse>
          <specular>1 1 1 1</specular>
          <pbr>
            <metal>
              <metalness>0.5</metalness>
              <roughness>0.5</roughness>
            </metal>
          </pbr>
        </material>
      </visual>
    </link>
    <joint name='rotor_joint' type='revolute'>
      <child>rotor_link</child>
      <parent>tower_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <damping>0.004</damping>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>
  </model>
</sdf>
